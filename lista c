#include <stdio.h>
#include <stdlib.h>
int main(void) {

  printf("Digite o salario do funcionario:");
  float SalarioBaseB;
  scanf("%f", &SalarioBaseB);

  printf("Digite o numero de dependentes do funcionario:");
  int DependentesD;
  scanf("%d", &DependentesD);

  printf("Digite o numero de anos de casa do funcionario:");
  int AnosDeCasaC;
  scanf("%d", &AnosDeCasaC);

  printf("Digite o numero de anos atual:");
  int AnoAtuaA;
  scanf("%d", &AnoAtuaA);
}
